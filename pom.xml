            </activation>
        </profile>
        <profile>
            <id>prod</id>
            <properties>
                <environment>prod</environment>
            </properties>
        </profile>
        <profile>
            <id>hd</id>
            <properties>
                <environment>hd</environment>
            </properties>
        </profile>
